{
    "name": "krafthaus/bauhaus",
    "description": "The missing Laravel 4 Admin Generator",
    "keywords": ["admin generator", "cms", "admin", "krafthaus", "laravel", "backend", "form", "forms"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "KraftHaus",
            "email": "hello@krafthaus.nl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "intervention/image": "2.*",
		"maatwebsite/excel": "1.*"
    },
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"orchestra/testbench": "2.2.*"
	},
    "autoload": {
        "classmap": [
            "src/controllers",
			"src/commands"
        ],
        "psr-0": {
            "KraftHaus\\Bauhaus": "src/"
        }
    },
	"suggest": {
		"krafthaus/bauhaususer": "Add user capabilities to bauhaus",
		"krafthaus/bauhausblock": "Simple block handling for laravel"
	},
    "minimum-stability": "stable"
}
